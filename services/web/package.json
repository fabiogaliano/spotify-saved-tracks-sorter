{
	"name": "v0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"prepare": "cd ../.. && lefthook install",
		"dev": "react-router dev",
		"dev:full": "concurrently -n web,worker,ws,vector -c green,blue,magenta,yellow \"bun dev\" \"bun run worker\" \"bun run websocket\" \"bun run dev:vector\" | tee logs/all-batch.log",
		"dev:vector": "cd ../vectorization && uv run python api.py",
		"worker": "bun run workers/analysisWorker.ts",
		"websocket": "bun run workers/websocketServer.ts",
		"build": "react-router build",
		"build:prod": "NODE_ENV=production react-router build",
		"start": "react-router-serve ./build/server/index.js",
		"start:prod": "NODE_ENV=production react-router-serve ./build/server/index.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"lyrics:snapshot": "bun run app/lib/services/lyrics/utils/__tests__/generate-snapshots.ts",
		"format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"typecheck": "react-router typegen && tsc",
		"db:types": "rm -rf ./app/types/database.types.ts && bunx supabase gen types --lang=typescript --project-id 'szhpoywveuvfbhcxiafi' --schema public > app/types/database.types.ts",
		"clean": "rm -rf node_modules bun.lockb build .next .react-router",
		"clean:cache": "rm -rf node_modules/.cache .eslintcache",
		"clean:install": "bun run clean && bun install",
		"clean:all": "bun run clean && bun run clean:cache && rm -rf logs"
	},
