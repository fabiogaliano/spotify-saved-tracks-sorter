		"test:watch": "vitest",
		"worker": "bun run app/workers/index.ts",
		"start-worker": "bun run scripts/analysisWorker.ts",
		"websocket": "bun run scripts/websocketServer.ts",
		"start-all": "concurrently -n dev,worker,ws -c green,blue,magenta \"bun dev\" \"bun run start-worker\" \"bun run websocket\" | tee logs/all.log"
	},
