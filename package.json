		"start": "react-router-serve ./build/server/index.js",
		"typecheck": "react-router typegen && tsc",
		"gendbtypes": "rm -rf ./app/types/database.types.ts && bunx supabase gen types --lang=typescript --project-id 'szhpoywveuvfbhcxiafi' --schema public > app/types/database.types.ts",
